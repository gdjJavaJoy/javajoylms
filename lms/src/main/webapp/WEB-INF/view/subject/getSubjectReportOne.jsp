				if($('#subjectBoardNo').val() == '') {
					Swal.fire('test');
			    } else if($('#memberId').val() == '') {
			    	Swal.fire('test');
				} else if($('#subjectReportCommentContent').val() == '') {
					Swal.fire('댓글 내용을 입력해주세요');
				} else {
					if(flag) {
						$('#addSubjectReportCommentForm').submit();
