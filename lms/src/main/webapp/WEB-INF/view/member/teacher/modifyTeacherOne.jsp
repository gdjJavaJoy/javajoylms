			type:'get'
			,url:'/lms/getAddr'
			,data:{keyword : $('#keyword').val()}
			,success:function(a) {
				console.log(a);
				console.log(typeof (a));
				var a2 = JSON.parse(a);
				console.log(typeof (a2));
				console.log(a2);

				let arr = a2.results.juso; // 주소배열
				console.log(arr);
				for (let i = 0; i < arr.length; i++) {
					var addr = (arr[i].jibunAddr).replace(/\s/gi, "");
